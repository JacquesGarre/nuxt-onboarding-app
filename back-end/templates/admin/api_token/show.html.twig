{% extends 'base.html.twig' %}

{% block title %}ApiToken{% endblock %}

{% block body %}
    <h1>ApiToken</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ api_token.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ api_token.name }}</td>
            </tr>
            <tr>
                <th>Token</th>
                <td>{{ api_token.token }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ api_token.createdAt ? api_token.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ api_token.updatedAt ? api_token.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Active</th>
                <td>{{ api_token.active ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>GetUsers</th>
                <td>{{ api_token.getUsers ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>PostUsers</th>
                <td>{{ api_token.postUsers ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>PatchUsers</th>
                <td>{{ api_token.patchUsers ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>PutUsers</th>
                <td>{{ api_token.putUsers ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>DeleteUsers</th>
                <td>{{ api_token.deleteUsers ? 'Yes' : 'No' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_api_token_index') }}">back to list</a>

    <a href="{{ path('app_api_token_edit', {'id': api_token.id}) }}">edit</a>

    {{ include('api_token/_delete_form.html.twig') }}
{% endblock %}
